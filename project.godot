[application]

config/name="PuzzleTree"
run/main_scene="res://addons/PuzzleTree/examples/basic/basic.tscn"

[autoload]

Directions="*res://addons/PuzzleTree/utils/directions.gd"
Inputs="*res://addons/PuzzleTree/utils/inputs.gd"

[editor]

script_templates_search_path="res://addons/PuzzleTree/script_templates/"

[editor_plugins]

enabled=PoolStringArray( "res://addons/PuzzleTree/plugin.cfg" )

[gui]

common/drop_mouse_on_gui_input_disabled=true

[physics]

common/enable_pause_aware_picking=true

[rendering]

quality/driver/driver_name="GLES2"
vram_compression/import_etc=true
vram_compression/import_etc2=false
